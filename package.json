{
  "name": "react-movies",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=8.9.3"
  },
  "dependencies": {
    "@babel/core": "7.2.2",
    "@svgr/webpack": "4.1.0",
    "array-sort-by": "^1.2.1",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "1.0.0",
    "dotenv": "^8.0.0",
    "dotenv-expand": "^5.1.0",
    "enzyme": "^3.10.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "5.12.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.1",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.1",
    "grid-styled": "^5.0.2",
    "history": "^4.9.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-localstorage-mock": "^2.4.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "mini-css-extract-plugin": "0.5.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.8.6",
    "react-app-polyfill": "^0.2.2",
    "react-burger-menu": "^2.6.10",
    "react-day-picker": "^7.3.0",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^16.8.6",
    "react-router-dom": "^4.3.1",
    "react-text-mask": "^5.4.3",
    "redux-zero": "^5.0.2",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "shortid": "^2.2.14",
    "sinon": "^7.3.2",
    "style-loader": "0.23.1",
    "styled-components": "^4.2.0",
    "terser-webpack-plugin": "1.2.2",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node --max_old_space_size=4096 scripts/build.js",
    "installation": "npm install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/config/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/config/__mocks__/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!scripts/*.js",
      "!src/index.js",
      "!src/components/**/styles.js",
      "!src/components/**/index.js",
      "!src/client/**/*.js",
      "!src/components/common/Icon/*.js",
      "!src/server/**/*.js",
      "!src/utils/*.js",
      "!src/styles/**/*.js",
      "!src/components/Routes/Routes.js"
    ],
    "reporters": [
      "default"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupEnzyme.js",
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "enzyme-adapter-react-16": "^1.14.0"
  }
}
